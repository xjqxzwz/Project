<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#d7d7d7" android:endColor="#d7d7d7" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="@color/hui" android:endColor="@color/hui"
                    android:centerColor="@color/hui" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="@color/hui" android:endColor="@color/hui"
                    android:centerColor="@color/hui" />
            </shape>
        </clip>
    </item>
</layer-list>